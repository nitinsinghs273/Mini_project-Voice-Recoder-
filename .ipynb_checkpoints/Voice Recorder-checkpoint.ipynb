   "source": [
    "import sounddevice as sd\n",
    "from scipy.io.wavfile import write"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "b419063c",
   "metadata": {},
   "outputs": [],
   "source": [
    "freq = 44100\n",
    "duration = 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "c1eed5ef",
   "metadata": {},
   "outputs": [],
   "source": [
    "recording = sd.rec(int(duration*freq), samplerate=freq, channels=2)\n",
    "sd.wait()\n",
    "write(\"recording0.wav\", freq, recording)"
   ]
  },
